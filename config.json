{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "香港落地",
      "default_mode": "rule"
    },
  "cache_file": {
    "enabled": true,
    "path": "/etc/sing-box/cache.db",
    "store_fakeip": false
    }
  },

 "dns": 
 {
   "servers": 
   [
     {"tag": "dns_direct", "address": "local", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "直连"},
     {"tag": "dns_proxy", "address": "tls://8.8.8.8", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "香港落地"},
     {"tag": "dns_proxy_jp", "address": "tls://8.8.8.8", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "日本"},
     {"tag": "dns_proxy_tw", "address": "tls://8.8.8.8", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "台湾"},
     {"tag": "dns_proxy_final", "address": "tls://8.8.8.8", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "香港落地","client_subnet":"39.88.213.1"}
   ],
   "rules": 
   [
     {"outbound": "any", "action": "route", "server": "dns_direct", "disable_cache": true},
     {"clash_mode": "Direct", "action": "route", "server": "dns_direct"},
     {"clash_mode": "Global", "action": "route", "server": "dns_proxy"},
     {"rule_set": "geosite-tiktok", "action": "route", "server": "dns_direct_jp"},
     {"rule_set": "geosite-bahamut", "action": "route", "server": "dns_direct_tw"},
     {"rule_set": "geosite-cn", "action": "route", "server": "dns_direct"},
     {"rule_set": "geosite-geolocation-!cn", "action": "route", "server": "dns_proxy"}
   ],
   "final": "dns_proxy_final",
   "strategy": "ipv4_only"
},

"inbounds": [
  {
    "type": "tun",
    "address": "172.19.0.1/30",
    "mtu": 9000,
    "auto_route": true,
    "auto_redirect": true,
    "strict_route": true,
    "stack": "system",
    "platform": {
      "http_proxy": {
        "enabled": true,
        "server": "127.0.0.1",
        "server_port": 2080
      }
    }
  },
  {
    "type": "mixed",
    "listen": "127.0.0.1",
    "listen_port": 2080,
    "sniff": true,
    "users": []
  }
],

  "outbounds": [
    { "tag": "香港落地", "type": "selector", "outbounds": ["香港自动", "香港A", "香港B", "香港C" ] },
    { "tag": "YouTube", "type": "selector", "outbounds":  ["香港落地", "日本", "台湾" ,"美国" ,"直连"]},
    { "tag": "Microsoft", "type": "selector", "outbounds":  ["香港落地", "日本", "台湾" ,"美国" ,"直连"] },
    { "tag": "Telegram", "type": "selector", "outbounds":  ["香港落地", "日本", "台湾" ,"美国" ,"直连"] },
    { "tag": "游戏平台", "type": "selector", "outbounds":  ["香港落地", "日本", "台湾" ,"美国" ,"直连"] },
    { "tag": "国外综合", "type": "selector", "outbounds":  ["香港落地", "日本", "台湾" ,"美国" ,"直连"] },
    { "tag": "漏网之鱼", "type": "selector", "outbounds": ["香港落地", "日本", "台湾" ,"美国" ,"直连"] },
    { "tag": "机场节点", "type": "selector", "outbounds": ["香港机场A", "香港机场B", "香港机场C", "日本机场A", "日本机场B", "日本机场C","美国机场A", "美国机场B", "美国机场C"] },
    { "tag": "香港自动", "type": "urltest", "outbounds": ["香港A","香港B","香港C"], "url": "http://www.gstatic.com/generate_204", "interval": "1m", "tolerance": 100 },
    { "tag": "GLOBAL", "type": "selector", "outbounds": ["香港A","香港B","香港C","日本", "台湾" ,"美国","香港机场A", "香港机场B", "香港机场C", "日本机场A", "日本机场B", "日本机场C","美国机场A", "美国机场B", "美国机场C"]},
    { "tag": "直连", "type": "direct" }
  ],
  
  "route": 
 {
   "auto_detect_interface": true,
   "final": "漏网之鱼",
   "rules": 
   [
     {"action": "sniff"},
     {"protocol": "dns", "action": "hijack-dns"},
     {"protocol": "bittorrent", "action": "route", "outbound": "直连"},
     {"clash_mode": "direct", "action": "route", "outbound": "直连"},
     {"clash_mode": "global", "action": "route", "outbound": "GLOBAL"},
     {"rule_set": ["geosite-private","geoip-private"], "action": "route", "outbound": "直连"},
     {"rule_set": ["geosite-telegram", "geoip-telegram"], "action": "route", "outbound": "Telegram"},
     {"rule_set": "geosite-youtube", "action": "route", "outbound": "YouTube"},
     {"rule_set": "geosite-tiktok", "action": "route", "outbound": "日本"},
     {"rule_set": "geosite-bahamut", "action": "route", "outbound": "台湾"},
     {"rule_set": ["geosite-onedrive","geosite-microsoft"], "action": "route", "outbound": "Microsoft"},
     {"rule_set": ["geosite-epic","geosite-steam","geosite-blizzard"], "action": "route", "outbound": "游戏平台"},
     {"rule_set": "geosite-geolocation-!cn", "action": "route", "outbound": "国外综合"},
     {"rule_set": ["geoip-cn", "geosite-cn"], "action": "route", "outbound": "直连"}
   ],
    
   "rule_set": [
    { "tag": "geosite-youtube", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs", "download_detour": "香港落地" },
